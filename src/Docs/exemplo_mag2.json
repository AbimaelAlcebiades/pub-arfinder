    		"NormalizedTitle",
    		"DOI",
            "OriginalVenue",
            "NormalizedVenue",
            "Keywords",
            "PublishYear",
            "PublishDate",
            "CitationCount",
            "ConferenceSeriesID",
            "JournalID",
            "AuthorsIDs",
            "AffiliationIDs",
            "AuthorSequenceNumbers",
            "FieldOfStudyIDs",
            "CitationIDs"
        ]
    },
    "objeto2": {
       "type": "Author",
        "select": [ "DisplayAuthorName" ]
    }
}
